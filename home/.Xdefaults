URxvt.termName: rxvt-256color
URxvt.buffered: true
URxvt.cursorColor: yellow
URxvt.underlineColor: green
URxvt.scrollBar: false
URxvt.saveLines: 10000

!URxvt.font:xft:Bitstream Vera Sans Mono:pixelsize=12:antialias=true
!URxvt.font:xft:Liberation Mono:pixelsize=12:antialias=true
!URxvt.font:xft:Luxi Mono:pixelsize=12:antialias=true
!URxvt.font:xft:Monaco:pixelsize=12:antialias=true
!URxvt.font:xft:Anonymous Pro:antialias=true
URxvt.font:xft:Terminus:pixelsize=14:antialias=true

!------------
!True Transparency
!------------
!set to 32-bit for real transparency (compositing required (see xcompmgr)
!URxvt.depth: 32 transparent=0000 opaque=ffff
!URxvt.fading: 10

URxvt.secondaryScroll:  true    # Enable Shift-PageUp/Down in screen


URxvt.internalBorder: 0
URxvt.externalBorder: 0

! tangoesque scheme
*background: #050505
*foreground: #babdb6
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff


